{
  "expo": {
    "name": "gym",
    "slug": "gym-hub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/HUB.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/GYMHUB.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "googleServicesFile": "",
      "bundleIdentifier": "com.alberto.gymhub",
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "",
      "bundleIdentifier": "com.alberto.gymhub",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/HUB.png",
        "backgroundColor": "#E7BE60"
      },
      "package": "com.alberto.gymhub"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/HUB.png",
      "splash": {
        "image": "./assets/images/GYMHUB.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with others.."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "da109601-5d2a-4244-b09d-8268a875bad6"
      }
    },
    "owner": "pohblano"
  }
}