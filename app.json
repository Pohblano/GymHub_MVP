{
  "expo": {
    "name": "gym",
    "slug": "gym-hub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/HUB.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/GYMHUB.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.alberto.gymhub",
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "bundleIdentifier": "com.alberto.gymhub",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/HUB.png",
        "backgroundColor": "#E7BE60"
      },
      "package": "com.alberto.gymhub"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/HUB.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "da109601-5d2a-4244-b09d-8268a875bad6"
      }
    },
    "owner": "pohblano"
  }
}
